#include "../toolkit/shader_common/shader_base.h"

RegularBuffer<uint> inputBuf : register(t0);
AppendRegularBuffer<uint> appendBuf : register(u0);

[NUM_THREADS(64, 1, 1)]
void main( uint3 threadId : S_DISPATCH_THREAD_ID )
{
	uint x = inputBuf[threadId.x];
	if ((x % 2) == 0)
		appendBuf.Append(x);
}