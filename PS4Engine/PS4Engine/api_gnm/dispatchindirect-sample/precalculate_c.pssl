Texture2D<float> g_texture : register(t0);
RW_DataBuffer<uint> g_indirect : register(u0);

[NUM_THREADS(1, 1, 1)]
void main()
{
	uint2 dimensions;
	g_texture.GetDimensions(dimensions.x, dimensions.y);
	g_indirect[0] = dimensions.x / 8;
	g_indirect[1] = dimensions.y / 8;
	g_indirect[2] = 1;
}