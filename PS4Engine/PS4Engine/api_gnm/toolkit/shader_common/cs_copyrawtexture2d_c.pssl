#include "shader_base.h"

RW_Texture2D<int4> DestTex : register(u0);
Texture2D<int4> SourceTex : register(t0);

[NUM_THREADS(8,8,1)]
void main( uint3 position : S_DISPATCH_THREAD_ID )
{
	DestTex[position] = SourceTex.Load(int3(position.x, position.y, 0));
}

